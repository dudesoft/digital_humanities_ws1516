<?xml version="1.0"?>
<QCARConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:noNamespaceSchemaLocation="qcar_config.xsd">
   <Tracking>
      <ImageTarget name="FlakesBox.Front" size="90 120" />
      <ImageTarget name="FlakesBox.Top" size="90 60" />
      <ImageTarget name="FlakesBox.Bottom" size="90 60" />
      <ImageTarget name="FlakesBox.Left" size="60 120" />
      <ImageTarget name="FlakesBox.Right" size="60 120" />
      <ImageTarget name="FlakesBox.Back" size="90 120" />
      <MultiTarget name="FlakesBox">
         <Part name="FlakesBox.Front" rotation="AD: 1 0 0 0" translation="0 0 30" />
         <Part name="FlakesBox.Back" rotation="AD: 0 1 0 180" translation="0 0 -30" />
         <Part name="FlakesBox.Left" rotation="AD: 0 1 0 -90" translation="-45 0 0" />
         <Part name="FlakesBox.Right" rotation="AD: 0 1 0 90" translation="45 0 0" />
         <Part name="FlakesBox.Top" rotation="AD: 1 0 0 -90" translation="0 60 0" />
         <Part name="FlakesBox.Bottom" rotation="AD: 1 0 0 90" translation="0 -60 0" />
      </MultiTarget>
   </Tracking>
</QCARConfig>
